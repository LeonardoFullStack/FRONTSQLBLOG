<%- include('partials/header.ejs') %>
<main>
<h1>
    <%= title %>
</h1>
<p>
    <%= msg %>
</p>
<%= data.title %>
<form method="post" action="/admin/edit">
    <input type="text" name="title" placeholder="título" value="<%= data.title %>">
    <input type="hidden" name="oldTitle" placeholder="título" value="<%= data.title %>">
    <input type="text" name="extract" placeholder="Estracto" value="<%= data.extract %>">
    <input type="text" name="content" placeholder="Contenido" value="<%= data.content %>">
    <input type="text" name="entryImage" placeholder="Imagen" value="<%= data.entryimage %>">
    <input type="text" name="category" placeholder="Categoría" value="<%= data.category %>">
    <input type="submit" value="Publicar">

</form>
</main>
<%- include('partials/footer.ejs') %>